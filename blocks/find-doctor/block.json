{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "global360blocks/find-doctor",
	"title": "Find Doctor",
	"category": "360-blocks",
	"icon": "admin-users",
	"description": "A two-column layout with image upload and Find a Doctor button.",
	"keywords": ["doctor", "find", "two-column", "image"],
	"textdomain": "global360blocks",
	"attributes": {
		"imageUrl": {
			"type": "string",
			"default": ""
		},
		"imageId": {
			"type": "number",
			"default": 0
		},
		"heading": {
			"type": "string",
			"default": ""
		},
		"bodyText": {
			"type": "string",
			"default": ""
		}
	},
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"anchor": true,
		"className": true,
		"reusable": true,
		"inserter": true,
		"multiple": true,
		"lock": false
	},
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css"
}
