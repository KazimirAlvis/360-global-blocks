{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "global360blocks/two-column-cta",
	"version": "1.0.0",
	"title": "Two Column CTA",
	"category": "360-blocks",
	"icon": "columns",
	"description": "Two column layout with rich text content on left and button on right.",
	"example": {},
	"supports": {
		"html": false,
		"anchor": true,
		"align": ["wide", "full"],
		"spacing": {
			"margin": true,
			"padding": true
		},
		"color": {
			"background": true,
			"text": false
		}
	},
	"attributes": {
		"heading": {
			"type": "string",
			"default": ""
		},
		"buttonText": {
			"type": "string",
			"default": "Take Risk Assessment Now"
		},
		"buttonUrl": {
			"type": "string",
			"default": ""
		},
		"backgroundColor": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "360-global-blocks",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
